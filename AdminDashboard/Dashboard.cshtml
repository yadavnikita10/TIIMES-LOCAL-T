@model TuvVision.Models.Dashboard
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_TuvVision.cshtml";
}


<style>
    td.dashboard-head {
        background: #e6e8ec;
        color: #000;
        font-weight: 600;
    }

    table.table-bordered.dashboard tr td {
        padding: 15px;
    }

    table.table-bordered.dashboard tbody tr td {
        border: 1px solid #000 !important;
    }

    td.dashboard-head {
        width: 380px;
        font-size: 14px;
    }

    td.dasboard-value {
        /*width: 80px;*/
        width: -20px;
    }
</style>
<script src="~/AllJsAndCss/js/Dashboardmin.js"></script>
<link href="~/AllJsAndCss/Dashboardjqueri-ui.css" rel="stylesheet" />
