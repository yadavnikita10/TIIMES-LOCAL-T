<div class="main-content3 xs-max-height">

    <!--Start of header-->
    <div class="container-fluid">
        <div class="row assignfixedwidth rp-15">



            <div class="col-xs-12">
                @*<hr class="custom-hr">*@
            </div><!-- /.col -->
            <!--Table was here-->

        </div><!-- /.row -->

        <div id="tabs">

            <ul>
                <li><a href="#tabs-2">Notification</a></li>
                <li><a href="#tabs-1">Performance</a></li>

            </ul>

            <div id="tabs-2">
                @foreach (var item in ViewBag.Quotation as IEnumerable<TuvVision.Models.ListQuotationDashboard>)
                {
                    <div class="row">
                        <div class="col-sm-12 col-md-12 boxBdr">


                            <table class="table-bordered dashboard">
                                <tbody>


                                    <tr>
                                        @if (item.Role == "1")
                                        {
                                            if (item.SendForApprovel != "Auto" && item.SendForApprovel != "")
                                            {
                                                if (item.RegretStatus == "Regret")
                                                {

                                                }
                                                else
                                                {
                                                    <p>(@item.CreatedDate) @item.Name  has Sent you request to approve cost sheet of quotation <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber</span></a> for @item.CompanyName . </p>
                                                }
                                            }

                                            if (item.RegretStatus == "Regret")
                                            {
                                                if (item.RegretReason == "")
                                                {
                                                    <p>(@item.CreatedDate)  @item.Name has regretted the enquiry number <a href="@Url.Action("Enquiry", "EnquiryMaster", new { EQ_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.EnquiryNumber</span></a> for @item.CompanyName</p>
                                                }
                                                else
                                                {
                                        <p>(@item.CreatedDate)  @item.Name has regretted the enquiry number <a href="@Url.Action("Enquiry", "EnquiryMaster", new { EQ_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.EnquiryNumber for </span></a> @item.CompanyName with reason @item.RegretReason </p>
                                                }
                                            }
                                            if (item.QuotationStatus == "3")//Won
                                            {
                                                <p>(@item.CreatedDate)  @item.Name bagged order against quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName</p>

                                            }
                                            if (item.QuotationStatus == "4")//Lost
                                            {
                                                <p>(@item.CreatedDate)  We have not received order against Quotation Number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber</span></a> for @item.CompanyName created by  @item.Name due to reason @item.LostReason.  </p>

                                            }
                                            if (item.CHComment != "" && item.CHComment != null)
                                            {
                                                <p>(@item.CHCommentDate) @item.ClusterHeadName has commented on cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber</span></a> for @item.CompanyName created by @item.Name as @item.CHComment.</p>
                                            }

                                            if (item.CostSheetCHStatus == "2")
                                            {
                                                <p>(@item.ApprovedOrNotApprovedDateCH) @item.ClusterHeadName has approved the cost sheet of Quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue">@item.QuotationNumber</span></a> for @item.CompanyName created by @item.Name .</p>
                                            }
                                            if (item.SendForApprovel == "Auto")
                                            {
                                                <p>(@item.SendForApprovelDate) Tiimes has auto - approved the quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName  created by @item.Name.</p>
                                            }
                                            if (item.CostSheetCHStatus == "1")
                                            {
                                                <p>(@item.ApprovedOrNotApprovedDateCH) @item.ClusterHeadName has not approved the cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber</span></a> for @item.CompanyName Created by @item.Name .</p>
                                            }

                                        }
                                        else if (item.Role == "46" || item.Role == "47" || item.Role == "48" || item.Role == "49" || item.Role == "51")
                                        {
                                            if (item.RegretStatus == "Regret")
                                            {

                                            }
                                            else
                                            {
                                                if (item.SendForApprovel != "Auto" && item.SendForApprovel != "" && item.SendForApprovel != "NA")
                                                {
                                                    if (item.EstimatedAmount > 1000000)
                                                    {
                                                        <p>(@item.SendForApprovelDate) @item.Name has Sent you a quotation <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> @item.CompanyName for approval. </p>
                                                    }
                                                    else
                                                    {
                                                        <p>(@item.SendForApprovelDate) @item.Name has sent request to approve cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName to Mr. @item.BranchHeadName </p>
                                                    }
                                                }

                                            }


                                            if (item.RegretStatus == "Regret")
                                            {
                                                if (item.RegretReason == "")
                                                {
                                                    <p>(@item.CreatedDate)  @item.Name has regretted the enquiry number <a href="@Url.Action("Enquiry", "EnquiryMaster", new { EQ_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.EnquiryNumber </span></a> for @item.CompanyName</p>
                                                }
                                                else
                                                {
                                                    <p>(@item.CreatedDate) @item.Name has regretted the enquiry number <a href="@Url.Action("Enquiry", "EnquiryMaster", new { EQ_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.EnquiryNumber </span></a> for @item.CompanyName with reason @item.RegretReason </p>
                                                }

                                            }
                                            if (item.QuotationStatus == "3")
                                            {
                                                <p>(@item.CreatedDate)  @item.Name bagged order against quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber</span></a> for @item.CompanyName</p>

                                            }
                                            if (item.QuotationStatus == "4")//Lost
                                            {
                                                <p>(@item.CreatedDate)  We have not received order against Quotation Number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a>for @item.CompanyName created by  @item.Name due to reason @item.LostReason.  </p>

                                            }
                                            if (item.PCHComment != "" && item.PCHComment != null)
                                            {
                                                <p>(@item.PCHCommentDate)  @item.BranchHeadName has commented on cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName created by @item.Name as @item.PCHComment.</p>
                                            }

                                            if (item.CostSheetPCHStatus == "2")
                                            {
                                                if (item.PCHComment != "" && item.PCHComment != null)
                                                {
                                                    <p>(@item.ApprovedOrNotApprovedDatePCH) @item.BranchHeadName has approved the cost sheet of Quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a>for @item.CompanyName created by @item.Name with @item.PCHComment.</p>
                                                }
                                                else
                                                {
                                                    <p>(@item.ApprovedOrNotApprovedDatePCH) @item.BranchHeadName has approved the cost sheet of Quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber</span></a> for @item.CompanyName created by @item.Name .</p>
                                                }

                                            }
                                            if (item.CostSheetPCHStatus == "1")
                                            {
                                                if (item.PCHComment != "")
                                                {
                                                    <p>(@item.ApprovedOrNotApprovedDatePCH) @item.BranchHeadName has not approved the cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName Created by @item.Name with @item.PCHComment.</p>
                                                }
                                                else
                                                {
                                                    <p>(@item.ApprovedOrNotApprovedDatePCH) @item.BranchHeadName has not approved the cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName Created by @item.Name .</p>
                                                }

                                            }
                                            if (item.SendForApprovel == "Auto")
                                            {
                                                <p>(@item.SendForApprovelDate) Tiimes has auto - approved the quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName created by @item.Name.</p>
                                            }

                                        }
                                        else
                                        {
                                            if ((item.PCHComment != null) && (item.PCHComment != ""))
                                            {
                                                <p>(@item.PCHCommentDate)  @item.BranchHeadName has commented on cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName as @item.PCHComment .</p>


                                            }
                                            if (item.CHComment != "" && item.CHComment != null)
                                            {
                                                <p>(@item.CHCommentDate) @item.ClusterHeadName has commented on cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName as @item.CHComment . </p>
                                            }
                                            if (item.CostSheetPCHStatus == "2")
                                            {
                                                <p>(@item.ApprovedOrNotApprovedDatePCH) @item.BranchHeadName has approved the cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName.  @*created by @item.Name .*@</p>
                                            }
                                            if (item.CostSheetCHStatus == "2")
                                            {
                                                <p>(@item.ApprovedOrNotApprovedDateCH) @item.ClusterHeadName has approved the cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName.  @*created by @item.Name .*@</p>
                                            }
                                            if (item.SendForApprovel == "Auto")
                                            {
                                                <p>(@item.SendForApprovelDate) Tiimes has auto - approved the quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber  </span></a> for @item.CompanyName created by @item.Name.</p>
                                            }
                                            if (item.CostSheetPCHStatus == "1")
                                            {
                                                if (item.PCHComment != "")
                                                {
                                                    <p>(@item.ApprovedOrNotApprovedDatePCH) @item.BranchHeadName has not approved the cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName Created by @item.Name with @item.PCHComment.</p>
                                                }
                                                else
                                                {
                                                    <p>(@item.ApprovedOrNotApprovedDatePCH) @item.BranchHeadName has not approved the cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName Created by @item.Name .</p>
                                                }

                                            }
                                            if (item.CostSheetCHStatus == "1")
                                            {
                                                <p>(@item.ApprovedOrNotApprovedDateCH) @item.ClusterHeadName has not approved the cost sheet of quotation number <a href="@Url.Action("Quotation", "QuotationMaster", new { PK_QM_ID = @item.PK_QM_ID })" title="Edit"><span style="color:blue"> @item.QuotationNumber </span></a> for @item.CompanyName Created by @item.Name .</p>
                                            }

                                        }


                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                }



