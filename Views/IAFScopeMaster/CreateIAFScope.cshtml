@model TuvVision.Models.IAFScopeMaster
@{
    ViewBag.Title = "CreateIAFScope";
    Layout = "~/Views/Shared/_TuvVision.cshtml";
}


<div class="main-content3 xs-max-height grey-bg-f3">
    <div class="grey-bg-f3">
        <!--Start of header-->
        <div class="container-fluid">
            <div class="row assignfixedwidth rp-5">

                <div class="col-xs-12 form-col">
                    <div class="row">

                        <div class="col-xs-12 text-right">
                            <a href="@Url.Action("ListIAFScopeMaster", "IAFScopeMaster")" class="btn btn-info dynamic-function-btn"><i class="fa fa-long-arrow-left"></i> Go Back</a>
                        </div>

                        <div class="col-xs-12">
                            <div class="white-bg section-box-shadow">
                                <h3 class="form-page-title">Create IAF Scope</h3>
                                <div class="row">
                                    <div class="col-xs-12 col-sm-offset-1 col-sm-10">
                                        <div class="form-box">
                                            <form method="post" enctype="multipart/form-data">

                                                <div class="row">

                                                    <div class="col-xs-12 col-sm-4">
                                                        <div class="form-group">
                                                            <label>IAF Scope Name </label>
                                                            @Html.TextBoxFor(model => model.IAFScopeName, htmlAttributes: new { placeholder = "IAF Scope Name", @class = "form-control", autocomplete = "off" })


                                                            @Html.ValidationMessageFor(model => model.IAFScopeName, "", new { @class = "text-danger validation-msg" })

                                                        </div>

                                                    </div>

                                                    <div class="col-xs-12 col-sm-4">
                                                        <div class="form-group">
                                                            <label>IAF Scope Number </label>
                                                            @Html.TextBoxFor(model => model.IAFScopeNumber, new { @placeholder = "IAF Scope Number", @class = "form-control", autocomplete = "off" })
                                                            @Html.ValidationMessageFor(model => model.IAFScopeNumber, "", new { @class = "text-danger validation-msg" })
                                                        </div>
                                                    </div>
                                                </div>

                                                

                                                <div class="row">
                                                    <div class="col-xs-12 col-sm-12">
                                                        <div class="form-group">
                                                            <button type="reset" class="btn btn-default insert-form-btn"> Reset </button>
                                                            <button type="submit" class="btn btn-default insert-form-btn"> Save </button>
                                                        </div>
                                                    </div>
                                                </div>


                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<script type="text/javascript" language="javascript" src="~/AllJsAndCss/datatable/js/jquery.dataTables.js"></script>
<script src="~/AllJsAndCss/datatable/js/jquery.dataTables.js"></script>
<script src="~/AllJsAndCss/js/datatable-custom.js"></script>




