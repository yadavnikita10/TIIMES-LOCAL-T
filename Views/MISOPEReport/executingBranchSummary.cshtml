@*calender control*@






@using (Html.BeginForm("ExecutingBranchSummary", "MISOPEReport", FormMethod.Post, new { enctype = "multipart/form-data", @id = "form1" }))
{
    <div class="main-content3 xs-max-height">
        <div class="container-fluid">

            <!--Start of header-->
            @*<a href="@Url.Action("Welcome", "Login")" class="btn btn-info dynamic-function-btn"><i class="fa fa-long-arrow-left"></i> Go Back</a>*@


           

                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-10">
                            <a href="@Url.Action("Welcome","Login")" class="logo">
                                <img src="~/AllJsAndCss/images/logoN.png" alt="" style="max-width: 210%;margin-top: -59px;">
                            </a>
                            <h3 class="dynamic-page-title dynamic-page-title-2  text-left" style="color: #001ED2;font-size:23px;font-weight:bold;padding-left:147px;padding-top:10px;font-family:'TNG Pro'; ">TUV India Private Limited <br />Call Analysis - Executing Branch </h3>
                        </div>

                        <div class="col-lg-2" style="padding-right:20px;">
                            <button type="button" class="btn btn-info" style="margin-top: 16px;margin-left: 37px;width: 150px;background-color: #001ED2;color:white;border-radius: 0;" onclick="Redirect()">Go Back</button>

                        </div>
                    </div>
                </div>

                <div class="col-xs-12">


                    @*<button type="button" value="ExportToExcel" class="btn btn-info dynamic-function-btn dynamic-function-btn-2" onclick="location.href='@Url.Action("ExportIndex2", "MISOPEReport")'"
                    style="border:none;">
                Export To Excel
            </button>*@

                 
                    <div class="row">
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label>From Date:</label>
                                @Html.TextBoxFor(model => model.FromDate, "{0:dd MMM yyyy}", htmlAttributes: new { placeholder = "Select Start Date", @class = "form-control form-control-custom FromD", autocomplete = "off" })
                                @Html.ValidationMessageFor(model => model.FromDate, "", new { @class = "text-danger validation-msg" })
                            </div>
                        </div>


                        <div class="col-xs-4">
                            <div class="form-group">
                                <label>To Date:</label>
                                @Html.TextBoxFor(model => model.ToDate, "{0:dd MMM yyyy}", htmlAttributes: new { placeholder = "Select To Date", @class = "form-control form-control-custom ToD", autocomplete = "off" })
                                @Html.ValidationMessageFor(model => model.ToDate, "", new { @class = "text-danger validation-msg" })
                            </div>

                        </div>

                        <div class="col-xs-2">
                            <div class="form-group">
                                <button type="submit" class="btn btn-info dynamic-function-btn dynamic-function-btn-2" style="margin-top: 22px;">Search</button>

                            </div>

                        </div>

                    </div>

                </div>


                <br />


         


            <div class="form-group" style="padding-left:25px;">
                <hr class="custom-hr1">
            </div>


            <div class="col-lg-12">
                @*<div class="col-xs-12" style="overflow:auto;height:500px;width:100%">
                    <table id="example" class="display dataTable no-footer" style="width:180%;">*@
                <div class="col-xs-12 table-container">
                    <table class="display dataTable no-footer fixed-size-table table-striped" id="example">
                        <thead>
                            <tr style="background-color: #104193; font-size:13px; color: #ffffff; text-align:right;">
                                <th style="width:80px;">Branch Name</th>
                                <th>Excecuted calls </th>
                                <th>Own</th>
                                <th>%</th>
                                <th>Other branch</th>
                                <th>%</th>
                                <th>ontime</th>
                                <th>%</th>
                                <th>Ontime own calls</th>
                                <th>%</th>
                                <th>Ontime other branches calls</th>
                                <th>%</th>
                                <th>delayed</th>
                                <th>%</th>
                                <th>Delayed own calls</th>
                                <th>%</th>
                                <th>Delayed other branches calls</th>
                                <th>%</th>
                                <th>Competant inspector assigned</th>
                                <th>%</th>
                                <th>Call assigned to inspector</th>
                                <th>Call Excecuted/inspector</th>
                                <th class="hidden"></th>

                            </tr>
                        </thead>
                        @{
                            var items = ViewData["ListData"] as IList<TuvVision.Models.CallsModel>;


                            if (items != null)
                            {

                                <tbody>
                                    @foreach (var obj in ViewData["ListData"] as IEnumerable<TuvVision.Models.CallsModel>)
                                    {
                                        <tr>
                                            <td>@obj.Branch_Name</td>
                                            <td>@obj.TotalCalls</td>
                                            <td>@obj.Own</td>
                                            <td>@obj.OwnPercentage</td>
                                            <td>@obj.Other</td>
                                            <td>@obj.OtherPercentage</td>
                                            <td>@obj.Ontime</td>
                                            <td>@obj.OnTimePercentage</td>
                                            <td>@obj.OntimeOwnCalls</td>
                                            <td>@obj.OntimeOwnCallsPercentage</td>
                                            <td>@obj.OntimeOtherCalls</td>
                                            <td>@obj.OntimeOtherCallsPercentage</td>
                                            <td>@obj.DelayCount</td>
                                            <td>@obj.DelayCountPercentage</td>
                                            <td>@obj.DelayedOwnCalls</td>
                                            <td>@obj.DelayedOwnCallsPercentage</td>
                                            <td>@obj.DelayedOtherCalls</td>
                                            <td>@obj.DelayedOtherCallsPercentage</td>
                                            <td>@obj.competantinspectorassigned</td>
                                            <td>@obj.competantinspectorassignedPercentage</td>
                                            <td>@obj.inspectorassigned</td>
                                            <td>@obj.inspectorassignedPercentage</td>
                                        </tr>
                                    }


                                </tbody>
                            }
                        }

                        @if (Model.GrandTotal != null)
                        {
                            <tfoot>
                                <tr>
                                    <td style="background-color: lightgray;">@Model.GrandTotal.Branch_Name</td>
                                    <td style="background-color: lightgray;">@Model.GrandTotal.GrandTotalTotalCalls</td>
                                    <td style="background-color: lightgray;">@Model.GrandTotal.GrandOwn</td>
                                    <td style="background-color: lightgray;">@(Model.GrandTotal.GrandOwnPercentage.ToString("0.##"))</td>
                                    <td style="background-color: lightgray;">@Model.GrandTotal.GrandOther</td>
                                    <td style="background-color: lightgray;">@(Model.GrandTotal.GrandOtherPercentage.ToString("0.##"))</td>
                                    <td style="background-color: lightgray;">@Model.GrandTotal.GrandTotalOntime</td>
                                    <td style="background-color: lightgray;">@(Model.GrandTotal.GrandTotalOnTimePercentage.ToString("0.##"))</td>
                                    <td style="background-color: lightgray;">@Model.GrandTotal.GrandOntimeOwnCalls</td>
                                    <td style="background-color: lightgray;">@(Model.GrandTotal.GrandTotalOnTimePercentage.ToString("0.##"))</td>


                                    <td style="background-color: lightgray;">@Model.GrandTotal.GrandOntimeOtherCalls</td>
                                    <td style="background-color: lightgray;">@(Model.GrandTotal.GrandOntimeOtherCallsPercentage.ToString("0.##"))</td>
                                    <td style="background-color: lightgray;">@Model.GrandTotal.GrandTotalDelayCount</td>
                                    <td style="background-color: lightgray;">@(Model.GrandTotal.GrandTotalDelayCountPercentage.ToString("0.##"))</td>
                                    <td style="background-color: lightgray;">@Model.GrandTotal.GrandDelayedOwnCalls</td>
                                    <td style="background-color: lightgray;">@(Model.GrandTotal.GrandDelayedOwnCallsPercentage.ToString("0.##"))</td>
                                    <td style="background-color: lightgray;">@Model.GrandTotal.GrandDelayedOtherCalls</td>
                                    <td style="background-color: lightgray;">@(Model.GrandTotal.GrandDelayedOtherCallsPercentage.ToString("0.##"))</td>

                                    <td style="background-color: lightgray;">@Model.GrandTotal.GrandTotalcompetantinspectorassigned</td>
                                    <td style="background-color: lightgray;">@(Model.GrandTotal.GrandTotalcompetantinspectorassignedPercentage.ToString("0.##"))</td>
                                    <td style="background-color: lightgray;">@Model.GrandTotal.GrandTotalinspectorassigned</td>
                                    <td style="background-color: lightgray;">@(Model.GrandTotal.GrandTotalinspectorassignedPercentage.ToString("0.##"))</td>
                                </tr>
                            </tfoot>
                        }




                    </table>


                </div>

            </div>


        </div>



    </div>


                            }




<script>
function Redirect() {
        window.location.href = "@Url.Action("Welcome", "Login")"
        }
        </script>






