@model TuvVision.Models.CallsModel
@using NonFactors.Mvc.Grid;
@using System.Web.Mvc;
@{
    ViewBag.Title = "executingBranchSummary";
    //Layout = "~/Views/Shared/_TuvVision.cshtml";
    Layout = null;
}




 <!-- Include jQuery library -->
