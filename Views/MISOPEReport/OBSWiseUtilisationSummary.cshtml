                                                    <td style="text-align:center;width:120px;font-size:smaller;background-color:#ff7575;">@PHLeaveCount</td>
                                                }
                                                else if (Model.dtUserAttendance.Columns[colcnt].ColumnName.ToString().ToUpper() == "SELECTED PERIOD DAYS")
