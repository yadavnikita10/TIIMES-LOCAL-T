                                                    && Model.dtUserAttendance.Columns[colcnt].ColumnName.ToString().ToUpper() != "SAP EMPLOYEE CODE"
                                                    && Model.dtUserAttendance.Columns[colcnt].ColumnName.ToString().ToUpper() != "SELECTED PERIOD DAYS"
