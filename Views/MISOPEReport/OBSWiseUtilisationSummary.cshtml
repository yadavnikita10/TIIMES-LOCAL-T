<script type="text/javascript">
    $(function () {
        $('#BrAuditee1').multiselect({
            enableFiltering: true,
            includeSelectAllOption: true,
            enableCaseInsensitiveFiltering: true,
            maxHeight: 450, 
            buttonText: function(options, select) {
                var selectedOptions = $('#BrAuditee1 option:selected');
                
                if (selectedOptions.length === 0) {
                    return 'Selected: 0';
                } else if (selectedOptions.length > 3) {
                    return selectedOptions.length + ' options selected';
                } else {
                    var selected = '';
                    selectedOptions.each(function () {
                        selected += $(this).text() + ', ';
                    });
                    return selected.substr(0, selected.length - 2); // Remove the trailing comma and space
                }
            }
        });

        // Retrieve and set selected options from localStorage on page load
        var selectedValues = localStorage.getItem('selectedValues');
        if (selectedValues) {
            $('#BrAuditee1').multiselect('select', selectedValues.split(','));
        }

        // Store selected options in localStorage when an option is selected
        $('#BrAuditee1').on('change', function () {
            var selectedOptions = $('#BrAuditee1 option:selected');
            var selectedValues = [];
            selectedOptions.each(function () {
                selectedValues.push($(this).val());
            });
            localStorage.setItem('selectedValues', selectedValues.join(','));
        });
    });

    
</script>
