<script>

    document.addEventListener('DOMContentLoaded', function () {
        const certDateCells = document.querySelectorAll('.cert-date');

        certDateCells.forEach(cell => {
            const certDateStr = cell.textContent.trim();
            if (certDateStr) {
                const certDateParts = certDateStr.split('/');
                const day = parseInt(certDateParts[0], 10);
                const month = parseInt(certDateParts[1], 10) - 1; // Months are zero-indexed in JavaScript Date
                const year = parseInt(certDateParts[2], 10);
                const certDate = new Date(year, month, day);
                const currentDate = new Date();

                // Calculate the date 30 days from the current date
                const oneMonthLaterDate = new Date();
                oneMonthLaterDate.setMonth(oneMonthLaterDate.getMonth() + 1);

                if (certDate <= oneMonthLaterDate && certDate > currentDate) {
                    // Apply orange color for dates within the next 30 days
                    cell.style.backgroundColor = 'orange';
                } else if (certDate <= currentDate) {
                    // Apply red color for dates that are in the past or today
                    cell.style.backgroundColor = 'red';
                } else {
                    // Clear any existing background color for dates more than 30 days in the future
                    cell.style.backgroundColor = '';
                }
            }
        });
    });
</script>








