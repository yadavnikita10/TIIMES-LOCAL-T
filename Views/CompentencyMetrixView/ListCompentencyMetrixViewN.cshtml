@model IEnumerable<TuvVision.Models.CompentencyMetrixView>

@{
    ViewBag.Title = "ListCompentencyMetrixViewN";
    Layout = "~/Views/Shared/_TuvVision.cshtml";
    int date;
    string formFilled = string.Empty;
}


<div class="main-content3 xs-max-height">

    <!--Start of header-->
    <div class="container-fluid">
        <div class="row assignfixedwidth rp-15">

            <div class="col-xs-12">
                <h3 class="dynamic-page-title dynamic-page-title-2  text-left">F-MR-16 R06 : Competency Matrix </h3>
                <button type="button" id="a" value="ExportToExcel" class="btn btn-info dynamic-function-btn dynamic-function-btn-2" onclick="location.href='@Url.Action("ExportIndex","CompentencyMetrixView")'">
                    Export To Excel
                </button>
            </div><!-- /.col -->

           
            <!--Table was here-->
            <div class="col-xs-12">
                <b>Legends for Authorization level</b>

                <table>
                    <tr>
                        <th style="font-size:smaller;">1.Can work under supervision.</th>
                        <th style="font-size:smaller;">2.Can work individually under guidance of seniors.</th>
                        <th style="font-size:smaller;">3.Can work independently, guide and monitor others.</th>
                        <th style="font-size:smaller;">4.Can train, monitor, guide & evaluate others.</th>
                    </tr>


                </table>
                </div>
                <div class="col-xs-12">
                    <hr class="custom-hr">
                </div><!-- /.col -->
            </div>

        
          
        
            

            <div class="col-xs-12">
                <table id="example" class="display" cellspacing="0" width="100%">
                    <thead>
                        <tr>                           
                            <th>PK_UserId</th>
                            <th>Candidate Name</th>
                            <th>Profile Data Completed</th>
                            <th>OBS Name</th>
                            <th>Core field of study profile</th>
                            <th>TechniqalCompetency Verify</th>

                            <th>Form Filled</th>
                            <th>Competent In Scope</th>
                            
                            @*<th>Employment Category</th>*@
                            <th>Stamp Number</th>
                            <th>Branch</th>
                            <th>Educational Qualification</th>
                            <th>Professional Qualification</th>
                            <th>Designation</th>
                            <th>Email ID</th>
                            <th>Mobile Number</th>
                            <th>Joining Date</th>
                            <th>Past Experience in YY:MM</th>
                            <th>Number Of Years With TUVI (Y & M)</th>
                            <th>Total Experience(Y & M)</th>
                            @foreach (var item in ViewData["Name"] as IEnumerable<TuvVision.Models.CompentencyMetrixView>)
                            {



                                <th>
                                    @item.CompentencyMetrixMasterName


                                </th>

                            }



                        </tr>
                    </thead>

                    <tbody>


                        @foreach (var b in Model)
                        {
                            if (b.strFormFilled == "")
                            {
                                formFilled = "NO";
                            }
                            else
                            {
                                if (b.strFormFilled == "0")
                                {
                                    formFilled = "NO";
                                }
                                else
                                {
                                    formFilled = "YES";
                                }
                            }


                            <tr>

                                <td>
                                    @*<a href="@Url.Action("CreateUser", "Users", new { @UserID = b.PK_userId })"
                                       target="_blank"
                                       style="margin-top:24px;">
                                        @b.PK_userId
                                    </a>*@
                                    @b.PK_userId


                                </td>
                                <td>

                                    @*<a href="@Url.Action("TechnicalCompetencyEvaluation", "TechnicalCompetencyEvaluation", new { @Id = b.PK_userId })"
                                       target="_blank"
                                       style="margin-top:24px;">
                                        @b.CandidateName
                                    </a>*@
                                    @b.CandidateName
                                </td>
                                <td>@b.ProfileDataCompleted</td>
                                <td>@b.OBSName</td>
                                <td>@b.CoreFieldOfStudy</td>
                                <td>@b.TechniqalCompetencyVerify</td>

                                <td>@formFilled</td>
                                <td>@b.CompetentInScope</td>
                                @*<td>@b.EmployeeCategory</td>*@
                                <td>@b.StampNumber</td>
                                <td>@b.Location</td>
                                <td>@b.EducationalQualification  </td>
                                <td>@b.AdditionalQualification  </td>
                                <td>@b.Designation               </td>
                                <td>@b.EmailId                   </td>
                                <td>@b.CellPhoneNumber           </td>
                                <td>@b.JoiningDate1            </td>
                                <td>@b.TotalExperienceInYears    </td>
                                <td>@b.TotalTUVExperience </td>
                                <td>@b.OverAllExp</td>

                                @{
                                    string[] ps;
                                    ps = b.AutharizeLevel.Split('#');//2,3


                                    foreach (string stock in ps)
                                    {
                                        <td>@stock </td>
                                    }
                                }




                            </tr>
                                    }
                    </tbody>

                </table>

            </div>
</div>

</div>

<script type="text/javascript" language="javascript" src="~/AllJsAndCss/datatable/js/jquery.dataTables.js"></script>

