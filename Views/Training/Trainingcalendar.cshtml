
@{
    ViewBag.Title = "Trainingcalendar";
    Layout = "~/Views/Shared/_TuvVision.cshtml";
}
<link href="~/AllJsAndCss/css/inner-pages.css" rel="stylesheet" />

<script src="~/AllJsAndCss/js/jquery.filterizr.js"></script>
<script>
        $(function () {
            $('.filtr-container').filterizr();
            $('.gallery-filter-tab li').click(function() {
            $(this).addClass('filtr-active');
            $(this).siblings().removeClass('filtr-active');
        });
        });
</script>
<!--Start of main content 2-->
<div class="main-content2 light-blue-bg">

    <!--Start of banner header-->
    <div class="container-fluid grey-bg-f3" style="background: url(/AllJsAndCss/images/training-banner.jpg);background-size: cover;background-repeat: no-repeat;position: relative;overflow: hidden;margin-bottom: 20px;">
        <div class="overlay-header"></div>
        <div class="row assignfixedwidth rp-20">
            <div class="col-xs-12 text-center">
                <div class="banner-header">
                    <h3><i class="fa fa-calendar"></i> TRAINING CALENDAR</h3>
                    <a href="@Url.Action("Training","Training")" class="btn btn-primary news-btn" style="margin:0;"> Go Back</a>
                </div>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
    <!--End of banner header-->
    <!--Start of gallery-->
    <div class="container-fluid light-blue-bg">
