        /* border: 2px solid #2d1c03; */
        background-color: #4e3105;
        color: #f5f5f7;
        position: relative;
    }


    .circle-one:before {
        position: absolute;
        content: "";
        width: 0px;
        height: 0px;
        border-top: 19px solid transparent;
        border-bottom: 19px solid transparent;
        border-left: 20px solid #f1f1f1;
        left: 0px;
        top: -1px;
    }

    .circle-one:after {
        position: absolute;
        content: "";
        width: 0px;
        height: 0px;
        border-top: 19px solid transparent;
        border-bottom: 19px solid transparent;
        border-left: 20px solid #f6a731;
        right: -18px;
        top: -1px;
    }


    .CircleRow .active .circle-one:before {
    }

    .CircleRow .active .circle-one:after {
        border-left: 20px solid #4e3105;
    }

    .CircleRow h4 {
        color: #847d7d;
    }

    .CircleRow .active h4 {
        color: #000000;
