@*@model IEnumerable<TuvVision.Models.QuizModel>*@ 
@model TuvVision.Models.QuizModel 
@{
    ViewBag.Title = "ResultList";
    Layout = "~/Views/Shared/_TuvVision.cshtml";
    int i = 0;
}

@using (Html.BeginForm())
{
    <div class="main-content3 xs-max-height grey-bg-f3">
        <div class="grey-bg-f3">
            <!--Start of header-->
            <div class="container-fluid">
                <div class="row assignfixedwidth rp-5">

                    <div class="col-xs-12 form-col">
                        <div class="row">
                            <div class="col-md-12 col-sm-12 col-xs-12 text-right">
                                <br />
                                <a href="@Url.Action("TrainingSchedule", "TrainingSchedule" , new {@UserID = Model.PK_TrainingScheduleId })" class="btn btn-default insert-form-btn"><i class="fa fa-long-arrow-left"></i> Go Back</a>

                                <a href="@Url.Action("ExportIndex1", "TrainingSchedule" , new {@PK_TrainingScheduleId = Model.PK_TrainingScheduleId })" class="btn btn-info dynamic-function-btn dynamic-function-btn-2" style="margin-top:15px;margin-left:3px;">Export To Excel</a>
                            </div>
                            <br />
                            <br />
                            <div class="col-xs-12">
                                <div class="white-bg section-box-shadow">
                                    <h3 class="form-page-title">Quiz Result</h3>
                                    <div class="row">
                                        <div class="col-xs-12 col-sm-offset-1 col-sm-10">
                                            <div class="form-box">
                                                <form>
                                                    <div class="row">
                                                        <div class="col-xs-12 col-sm-4">
                                                            @Html.HiddenFor(model => model.PK_TrainingScheduleId)
                                                        </div>
                                                    </div>

                                                    @*<h3 class="form-page-title">@ViewBag.TrngNm</h3>*@
                                                    <div>
                                                        @{
                                                            if (Convert.ToString(TempData["Success"]) != null && Convert.ToString(TempData["Success"]) != "")
                                                            {
                                                                <h5 style="color:green;font-weight:bold;">@Convert.ToString(TempData["Success"])</h5>
                                                            }
                                                            if (Convert.ToString(TempData["Deleted"]) != null && Convert.ToString(TempData["Deleted"]) != "")
                                                            {
                                                                <h5 style="color:red;font-weight:bold;">@Convert.ToString(TempData["Deleted"])</h5>
                                                            }
                                                            if (Convert.ToString(TempData["Error"]) != null && Convert.ToString(TempData["Error"]) != "")
                                                            {
                                                                <h5 style="color:red;font-weight:bold;">@Convert.ToString(TempData["Error"])</h5>
                                                            }
                                                        }
                                                    </div>

                                                    <div class="col-xs-12">
                                                        <table id="example" class="display" cellspacing="0" width="100%" >
                                                            <thead>
                                                                <tr>
                                                                    <th class="remove-search" >Sr.No</th>
                                                                    <th class="remove-search" >Trainee Name</th>
                                                                    <th class="remove-search">Mobile No</th>
                                                                    <th class="remove-search">Email</th>
                                                                    <th class="remove-search">Branch</th>
                                                                    <th class="remove-search" >Percent</th>
                                                                    <th class="remove-search">Attempt</th>
@*@foreach (var a in ViewBag.Question as IEnumerable<TuvVision.Models.QuizModel>)
{
    <th class="remove-search">@a.Question</th>
}*@
                                                                    
                                                                   
                                                                </tr>
                                                            </thead>

                                                            <tbody>
                                                              
                                                                @foreach (var v in ViewBag.ResultList as IEnumerable<TuvVision.Models.QuizModel>)
                                                                {
                                                                    i = i + 1;
                                                                    <tr>
                                                                        <td scope="row"> @i </td>
                                                                        <td>
                                                                            @v.UserID
                                                                        </td>
                                                                        <td>
                                                                            @v.Mobile
                                                                        </td>
                                                                        <td>
                                                                            @v.Email
                                                                        </td>
                                                                        <td class="remove-search">
                                                                            @v.Branch_Name
                                                                        </td>
                                                                        <td class="remove-search">
                                                                            @v.Percent
                                                                        </td>
                                                                        <td class="remove-search">
                                                                            @v.Attempt
                                                                        </td>
                                                                     @*@foreach (var b in ViewBag.Answer as IEnumerable<TuvVision.Models.QuizModel>)
                                                                     {
                                                                        <td class="remove-search">@b.Answer</td>
                                                                     }*@
                                                                    </tr>
                                                                }
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

<script type="text/javascript" language="javascript" src="~/AllJsAndCss/datatable/js/jquery.dataTables.js"></script>

<!--export to excel-->
<script type="text/javascript" language="javascript" src="~/AllJsAndCss/js/dataTables.buttons.min.js"></script>
<script type="text/javascript" language="javascript" src="~/AllJsAndCss/js/jszip.min.js"></script>
<script type="text/javascript" language="javascript" src="~/AllJsAndCss/js/buttons.html5.min.js"></script>
<script src="~/AllJsAndCss/js/datatable-custom2.js"></script>

